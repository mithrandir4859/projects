<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
	<entry key="password.regex">((?=.*\d)(?=.*[a-z])(?=.*[A-Z])(?=.*[@#$%]).*)</entry>
	<entry key="email.regex">^[_A-Za-z0-9-\+]+(\.[_A-Za-z0-9-]+)*@[A-Za-z0-9-]+(\.[A-Za-z0-9]+)*(\.[A-Za-z]{2,})$</entry>
	<entry key="firstname.minLength">3</entry>
	<entry key="firstname.maxLength">10</entry>
	<entry key="lastname.minLength">3</entry>
	<entry key="lastname.maxLength">10</entry>
	<entry key="phone.minLength">10</entry>
</properties>